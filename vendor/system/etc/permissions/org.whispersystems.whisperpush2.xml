<permissions>
    <!-- allow WhisperPush v2 to interact with incoming and outgoing SMS at a low level -->
    <allow-permission name="android.permission.READ_PHONE_BLACKLIST" signature="" />
    <allow-permission name="android.permission.INTERCEPT_SMS" signature="" />
    <allow-permission name="android.permission.MODIFY_PROTECTED_SMS_LIST" signature="" />
    <allow-permission name="android.permission.RECEIVE_PROTECTED_SMS" signature="" />
    <allow-permission name="android.permission.BROADCAST_SMS" signature="" />
    <allow-permission name="org.whispersystems.whisperpush.permission.UPGRADE" signature="" />
    <allow-permission name="com.cyngn.stats.SEND_ANALYTICS" signature="" />

    <feature name="org.whispersystems.whisperpush2" />
</permissions>
